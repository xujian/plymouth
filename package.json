{
  "name": "plymouth",
  "version": "0.0.0",
  "main": "desktop/main.js",
  "scripts": {
    "start": "npm-run-all -p open serve",
    "serve": "ng serve",
    "open": "wait-on tcp:4200 && npm run build:desktop && electron . --serve",
    "build:desktop": "tsc -p tsconfig.desktop.json",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "angular-svg-icon": "^14.0.0",
    "angular-svg-icon-preloader": "^4.0.1",
    "daisyui": "^2.46.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.7",
    "@angular/cli": "~14.2.7",
    "@angular/compiler-cli": "^14.2.0",
    "@types/jasmine": "~4.0.0",
    "autoprefixer": "^10.4.13",
    "electron": "^22.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-electron": "^7.3.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "tailwindcss": "^3.2.4",
    "typescript": "~4.7.2",
    "wait-on": "^7.0.1"
  }
}
